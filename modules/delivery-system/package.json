{
  "name": "delivery-system",
  "version": "1.0.0",
  "description": "Advanced content delivery engine for multi-platform distribution",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:platforms": "jest tests/platforms",
    "validate": "node -e \"console.log('Validating delivery system...'); require('./index.js'); console.log('âœ“ Delivery system validation passed');\"",
    "setup": "npm install && npm run validate",
    "lint": "eslint *.js platforms/*.js",
    "docs": "jsdoc -d docs *.js platforms/*.js"
  },
  "keywords": [
    "content-delivery",
    "multi-platform",
    "video-distribution",
    "social-media",
    "automation",
    "scheduling",
    "analytics",
    "youtube",
    "tiktok",
    "instagram",
    "twitter",
    "linkedin",
    "telegram",
    "discord",
    "cloud-storage",
    "backup",
    "notifications",
    "optimization",
    "retry-logic",
    "performance-monitoring"
  ],
  "dependencies": {
    "axios": "^1.6.0",
    "form-data": "^4.0.0",
    "googleapis": "^128.0.0",
    "instagram-basic-display-api": "^2.0.0",
    "twitter-api-v2": "^1.15.0",
    "linkedin-api": "^1.0.0",
    "telegraf": "^4.15.0",
    "discord.js": "^14.14.0",
    "aws-sdk": "^2.1490.0",
    "dropbox": "^10.34.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "sharp": "^0.32.6",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "moment-timezone": "^0.5.43",
    "retry": "^0.13.1",
    "p-limit": "^4.0.0",
    "progress": "^2.0.3",
    "mime-types": "^2.1.35",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.5.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "jsdoc": "^4.0.2",
    "@types/node": "^20.9.0"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2",
    "node-gyp": "^10.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auto-publish/delivery-system.git"
  },
  "bugs": {
    "url": "https://github.com/auto-publish/delivery-system/issues"
  },
  "homepage": "https://github.com/auto-publish/delivery-system#readme",
  "license": "MIT",
  "author": "Auto-Publish System",
  "contributors": [
    "AI Assistant"
  ],
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/auto-publish"
  },
  "config": {
    "platforms": {
      "supported": [
        "youtube",
        "tiktok",
        "instagram",
        "twitter",
        "linkedin",
        "telegram",
        "discord",
        "s3",
        "drive",
        "dropbox",
        "ftp",
        "email"
      ],
      "maxConcurrent": 5,
      "defaultTimeout": 300000,
      "retryAttempts": 3
    },
    "performance": {
      "maxFileSize": "8GB",
      "supportedFormats": [
        "mp4",
        "mov",
        "webm",
        "avi",
        "mkv"
      ],
      "estimatedCostPerDelivery": "$0.01-$0.50",
      "estimatedDurationPerPlatform": "30s-5min",
      "reliability": "96%"
    },
    "features": [
      "multi-platform-delivery",
      "scheduled-publishing",
      "automatic-optimization",
      "retry-mechanisms",
      "analytics-tracking",
      "notification-system",
      "backup-creation",
      "quality-control",
      "performance-monitoring",
      "cost-tracking",
      "batch-processing",
      "webhook-integration",
      "api-rate-limiting",
      "error-recovery",
      "progress-tracking"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
